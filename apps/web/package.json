{
  "name": "web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate:svelte": "svelte-kit sync",
    "generate": "concurrently pnpm:generate:*",
    "dev": "vite dev --port 3000 --open",
    "build": "pnpm generate && vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:markup": "markuplint --config ../../.markuplintrc \"**\"",
    "lint:js": "TIMING=1 eslint .",
    "lint:cspell": "cspell \"**\"",
    "lint:prettier": "prettier --check --ignore-path=../../.prettierignore .",
    "lint": "concurrently pnpm:lint:*",
    "test:watch": "vitest",
    "test": "vitest run",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.21",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "tailwind-preset-base": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
}
